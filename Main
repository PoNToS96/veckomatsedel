package elffors.veckomatsedel.projekt;

public class Main {

    public static void main(String[] args) {
        Matratt ratt1 = new Matratt("Korv och makaroner");
        Matratt ratt2 = new Matratt("Korvstroganoff", "Grädde, mjölk, korv");
        Matratt ratt3 = new Matratt("Fetaostsallad");
        Matratt ratt4 = new Matratt("Quorngryta", "Quorn, creme fraiche");
        Matratt ratt5 = new Matratt("Pannkakor", "Ägg, mjölk, mjöl");

        HanteraMatratter lista = new HanteraMatratter("Pontus matlista");
        HanteraMatratter lista2 = new HanteraMatratter("Herkules matlista");

        lista.laggTillMatratt(ratt1);
        lista.laggTillMatratt(ratt2);
        lista.laggTillMatratt(ratt3);
        lista.laggTillMatratt(ratt3);
        lista.laggTillMatratt(ratt4);
        lista.laggTillMatratt(ratt5);
        lista.laggTillMatratt(ratt5);
        lista.laggTillMatratt(ratt1);
        lista.skrivMatLista();

        lista2.laggTillMatratt(ratt5);
        lista2.laggTillMatratt(ratt5);
        lista2.laggTillMatratt(ratt1);
        lista2.skrivMatLista();

    }
}
